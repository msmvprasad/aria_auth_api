# Copy this file to `.env` and fill in the appropriate values for your
# environment.  These variables configure the database connection and
# Application Insights.

# MySQL connection details
DB_HOST=localhost
DB_PORT=3306
DB_USER=root
DB_PASSWORD=password
DB_NAME=mydatabase

# Application Insights connection string (or instrumentation key)
# Example: InstrumentationKey=00000000-0000-0000-0000-000000000000;IngestionEndpoint=https://<region>.in.applicationinsights.azure.com/
APPINSIGHTS_CONNECTION_STRING=

# The port on which the Express server will listen
PORT=3000