{
  "name": "express-typescript-knex-mysql2-appinsights",
  "version": "1.0.0",
  "description": "A simple Express.js server written in TypeScript using Knex.js with a MySQL database and integrated with Azure Application Insights.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "husky install",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "applicationinsights": "^3.7.0",
    "express": "^4.18.2",
    "knex": "^3.1.0",
    "mysql2": "^3.9.3",
    "joi": "^17.10.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.10",
    "supertest": "^6.3.3"
  }
}
